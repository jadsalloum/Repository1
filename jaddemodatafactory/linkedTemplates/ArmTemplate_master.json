{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "jaddemodatafactory"
		},
		"integrationRuntime_integrationRuntime-esamserver": {
			"type": "string",
			"metadata": "Integration runtime name",
			"defaultValue": "integrationRuntime-esamserver"
		},
		"integrationRuntime_integrationRuntime-jadlaptop": {
			"type": "string",
			"metadata": "Integration runtime name",
			"defaultValue": "integrationRuntime-jadlaptop"
		},
		"integrationRuntime_integrationRuntime1": {
			"type": "string",
			"metadata": "Integration runtime name",
			"defaultValue": "integrationRuntime1"
		},
		"integrationRuntime_integrationRuntimeAzureDataLake": {
			"type": "string",
			"metadata": "Integration runtime name",
			"defaultValue": "integrationRuntimeAzureDataLake"
		},
		"AzureDataLakeStore1_servicePrincipalKey": {
			"type": "secureString",
			"metadata": "Secure string for 'servicePrincipalKey' of 'AzureDataLakeStore1'"
		},
		"AzurePerfMonDataDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzurePerfMonDataDB'"
		},
		"FileServer1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'FileServer1'"
		},
		"SqlServer1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServer1'"
		},
		"AzureSqlSharepointDatabase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlSharepointDatabase'"
		},
		"AzureDataLakeStore1_properties_typeProperties_dataLakeStoreUri": {
			"type": "string",
			"defaultValue": "https://mydatalakestorage.azuredatalakestore.net/webhdfs/v1"
		},
		"AzureDataLakeStore1_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "b05a304e-cbf7-4ddf-a294-538c98dfc3f5"
		},
		"AzureDataLakeStore1_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "72f988bf-86f1-41af-91ab-2d7cd011db47"
		},
		"AzureDataLakeStore1_properties_typeProperties_subscriptionId": {
			"type": "string",
			"defaultValue": "ebb4f986-c8e7-475b-b0ac-34c473f39719"
		},
		"AzureDataLakeStore1_properties_typeProperties_resourceGroupName": {
			"type": "string",
			"defaultValue": "MSDN-AI-RG"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://JadsKeyVault.vault.azure.net/"
		},
		"FileServer1_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\temp\\PerfDatafor AI"
		},
		"FileServer1_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "middleeast\\jads"
		},
		"FileServer2_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\temp\\PerfDatafor AI"
		},
		"FileServer2_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "middleeast\\jads"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"integrationRuntime_integrationRuntime-esamserver": {
						"value": "[parameters('integrationRuntime_integrationRuntime-esamserver')]"
					},
					"integrationRuntime_integrationRuntime-jadlaptop": {
						"value": "[parameters('integrationRuntime_integrationRuntime-jadlaptop')]"
					},
					"integrationRuntime_integrationRuntime1": {
						"value": "[parameters('integrationRuntime_integrationRuntime1')]"
					},
					"integrationRuntime_integrationRuntimeAzureDataLake": {
						"value": "[parameters('integrationRuntime_integrationRuntimeAzureDataLake')]"
					},
					"AzureDataLakeStore1_servicePrincipalKey": {
						"value": "[parameters('AzureDataLakeStore1_servicePrincipalKey')]"
					},
					"AzurePerfMonDataDB_connectionString": {
						"value": "[parameters('AzurePerfMonDataDB_connectionString')]"
					},
					"AzureSqlSharepointDatabase_connectionString": {
						"value": "[parameters('AzureSqlSharepointDatabase_connectionString')]"
					},
					"FileServer1_password": {
						"value": "[parameters('FileServer1_password')]"
					},
					"SqlServer1_connectionString": {
						"value": "[parameters('SqlServer1_connectionString')]"
					},
					"AzureDataLakeStore1_properties_typeProperties_dataLakeStoreUri": {
						"value": "[parameters('AzureDataLakeStore1_properties_typeProperties_dataLakeStoreUri')]"
					},
					"AzureDataLakeStore1_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('AzureDataLakeStore1_properties_typeProperties_servicePrincipalId')]"
					},
					"AzureDataLakeStore1_properties_typeProperties_tenant": {
						"value": "[parameters('AzureDataLakeStore1_properties_typeProperties_tenant')]"
					},
					"AzureDataLakeStore1_properties_typeProperties_subscriptionId": {
						"value": "[parameters('AzureDataLakeStore1_properties_typeProperties_subscriptionId')]"
					},
					"AzureDataLakeStore1_properties_typeProperties_resourceGroupName": {
						"value": "[parameters('AzureDataLakeStore1_properties_typeProperties_resourceGroupName')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"FileServer1_properties_typeProperties_host": {
						"value": "[parameters('FileServer1_properties_typeProperties_host')]"
					},
					"FileServer1_properties_typeProperties_userId": {
						"value": "[parameters('FileServer1_properties_typeProperties_userId')]"
					},
					"FileServer2_properties_typeProperties_host": {
						"value": "[parameters('FileServer2_properties_typeProperties_host')]"
					},
					"FileServer2_properties_typeProperties_userId": {
						"value": "[parameters('FileServer2_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ArmTemplate_0')]"
			]
		}
	]
}